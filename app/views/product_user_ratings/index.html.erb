<p id="notice"><%= notice %></p>

<h1>Listing Product User Ratings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_user_ratings.each do |product_user_rating| %>
      <tr>
        <td><%= link_to 'Show', product_user_rating %></td>
        <td><%= link_to 'Edit', edit_product_user_rating_path(product_user_rating) %></td>
        <td><%= link_to 'Destroy', product_user_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product user rating', new_product_user_rating_path %>
